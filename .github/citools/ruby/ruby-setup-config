main() {
	printf "Setup Ruby Environment\n\n"

	print_ruler

	# printf "%s\n" "/root/.rustup/toolchains/stable-$(arch)-unknown-linux-gnu/bin" "/root/.cargo/bin" | tee -a "${GITHUB_PATH}"

	print_ruler

	echo Exit code: "${retval}"
